    "version": "0.2.0",
    "configurations": [
        {
            "cargo": {
                "args": [
                    "build",
                    "-p",
                    "migration-runner",
                    "-p",
                    "server",
                    "-p",
                    "dxp-todo-backend",
                ],
            },
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach",
            "program": "${workspaceFolder}/target/debug/dxp-todo-backend",
        },
    ]
}